pragma solidity ^0.8.20;

// Note: This is an example script showing how to verify a proof using a deployed HonkVerifier contract.

import {Script, console} from "forge-std/Script.sol";
import {HonkVerifier} from "../verifier.sol";

contract HonkVerifierScript is Script {
    HonkVerifier public verifier;

    function setUp() public {
        address verifier_address = vm.envAddress("VERIFIER_ADDRESS");
        verifier = HonkVerifier(verifier_address);
    }

    function _bytesToBytes32Array(bytes memory data) internal pure returns (bytes32[] memory out) {
        require(data.length % 32 == 0, "publicInputs: invalid length");
        uint256 n = data.length / 32;
        out = new bytes32[](n);
        for (uint256 i = 0; i < n; i++) {
            bytes32 word;
            assembly {
                word := mload(add(add(data, 0x20), mul(i, 0x20)))
            }
            out[i] = word;
        }
    }

    function run() public {
        // The proof
        bytes memory proof =
            hex"";

        // The public inputs
        bytes memory publicInputsRaw =
            hex"";
        bytes32[] memory publicInputs = _bytesToBytes32Array(publicInputsRaw);

        vm.startBroadcast();
        console.log("Verifying proof...");
        if (!verifier.verify(proof, publicInputs)) {
            revert("Proof verification failed");
        }
        console.log("Finished verification.");
        vm.stopBroadcast();
    }
}
